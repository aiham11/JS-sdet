// Fetch Jira response
String jiraApiEndpoint = ConfigLoader.getConfigValue("JIRA_API_ENDPOINT");
logger.info("Jira API Endpoint: " + jiraApiEndpoint);  // Add this line to log the endpoint
String jiraApiToken = ConfigLoader.getConfigValue("JIRA_API_TOKEN");
String jiraResponse = jiraOperation.fetchJiraNonMigratedTestcases(jiraApiEndpoint, jiraApiToken);
