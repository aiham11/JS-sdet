Map<String, String> attachmentMap = Utils.pharseJsonGetAttachmentUrlAndName(result);
if (attachmentMap != null && !attachmentMap.isEmpty()) {
    fileAttachmentDownloadPaths = Utils.downloadFileAttachmentFromJiraTestCase(attachmentMap,
                                tcFileAttachmentDownloadLocation, jiraApiKey, testcaseKey);
} else {
    logger.info("No attachments found for the test case: " + testcaseKey);
}
